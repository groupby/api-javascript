{
  "name": "groupby-api",
  "version": "0.0.3-development",
  "description": "Client for the GroupBy Searchandiser API.",
  "repository": {
    "type": "git",
    "url": "https://github.com/groupby/api-javascript.git"
  },
  "bugs": {
    "url": "https://github.com/groupby/api-javascript/issues"
  },
  "homepage": "https://github.com/groupby/api-javascript#readme",
  "author": "Ben Teichman <ben.teichman@groupbyinc.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module:esnext": "esnext/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/",
    "esnext/",
    "src/"
  ],
  "scripts": {
    "start": "start-runner -p re-start/presets/typescript",
    "esnext": "npm start build -- -- --compileOpts.configFile tsconfig.esnext.json --outDir esnext",
    "dev": "npm start dev",
    "test": "npm start test",
    "tdd": "npm start tdd",
    "prepush": "npm start prepush",
    "prepublish": "npm start build && npm run esnext"
  },
  "release": {
    "branch": "develop",
    "verifyConditions": "condition-circle"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "restart": {
      "testFiles": [
        "test/bootstrap.ts",
        "test/unit/**/*.ts"
      ],
      "releaseOpts": {
        "public": true
      }
    }
  },
  "devDependencies": {
    "@types/chai": "^4.0.0",
    "@types/mocha": "^2.2.41",
    "@types/sinon": "^2.3.1",
    "@types/sinon-chai": "^2.7.28",
    "chai": "^4.0.2",
    "condition-circle": "^1.5.0",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.13.4",
    "mocha": "^3.4.2",
    "mocha-suite": "^1.0.9",
    "re-start": "^1.14.8",
    "sinon": "^2.3.2",
    "sinon-chai": "^2.10.0",
    "start-simple-cli": "^4.1.1",
    "tslint": "^5.4.3",
    "tslint-config-groupby": "^1.0.0",
    "typescript": "^2.3.4",
    "typings": "^2.1.1",
    "xhr-mock": "^1.9.0"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/clone": "^0.1.30",
    "@types/deep-equal": "^1.0.0",
    "@types/qs": "^6.2.30",
    "axios": "^0.16.2",
    "clone": "^2.1.1",
    "deep-equal": "^1.0.1",
    "filter-object": "^2.1.0",
    "qs": "^6.1.0"
  }
}
